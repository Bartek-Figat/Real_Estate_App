{
  "compilerOptions": {
    "target": "es5",
      "module": "commonjs",
      "rootDir": "./",
      "strict": true,
      "sourceMap": true,
      "experimentalDecorators": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noImplicitAny": true,
      "alwaysStrict": true,
      "forceConsistentCasingInFileNames": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
  "files": ["types.d.ts"]
  }
